{
  "log": {
    "level": "error",
    "timestamp": true,
    "output": "./sing-box.log"
  },
  "dns": {
    "servers": [
      {
        "type": "https",
        "tag": "dns_proxy",
        "server": "1.1.1.1",
        "detour": "PROXY"
      },
      {
        "type": "https",
        "tag": "dns_direct",
        "server": "223.5.5.5"
      },
      {
        "type": "fakeip",
        "tag": "dns_fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "clash_mode": "Global",
        "server": "dns_proxy"
      },
      {
        "rule_set": "cn",
        "server": "dns_direct"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": "proxy",
        "server": "dns_fakeip"
      }
    ],
    "final": "dns_proxy",
    "strategy": "prefer_ipv4",
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "::",
      "listen_port": 1536
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "DIRECT",
      "routing_mark": 2025
    },
    {
      "type": "selector",
      "tag": "PROXY",
      "outbounds": [
        "HK",
        "TW",
        "JP",
        "SG",
        "US"
      ],
      "interrupt_exist_connections": true
    },
    {
      "type": "selector",
      "tag": "GLOBAL",
      "outbounds": [
        "PROXY"
      ]
    },
    {
      "type": "selector",
      "tag": "HK",
      "outbounds": [
      ]
    },
    {
      "type": "selector",
      "tag": "TW",
      "outbounds": [
      ]
    },
    {
      "type": "selector",
      "tag": "JP",
      "outbounds": [
      ]
    },
    {
      "type": "selector",
      "tag": "SG",
      "outbounds": [
      ]
    },
    {
      "type": "selector",
      "tag": "US",
      "outbounds": [
      ]
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "clash_mode": "Direct",
        "outbound": "DIRECT"
      },
      {
        "clash_mode": "Global",
        "outbound": "GLOBAL"
      },
      {
        "rule_set": "cn",
        "outbound": "DIRECT"
      },
      {
        "rule_set": "cnip",
        "outbound": "DIRECT"
      },
      {
        "ip_is_private": true,
        "outbound": "DIRECT"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "proxy",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/DustinWin/ruleset_geodata/releases/download/sing-box-ruleset/proxy.srs",
        "update_interval": "1d"
      },
      {
        "type": "remote",
        "tag": "cn",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/DustinWin/ruleset_geodata/releases/download/sing-box-ruleset/cn.srs",
        "update_interval": "1d"
      },
      {
        "type": "remote",
        "tag": "cnip",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/DustinWin/ruleset_geodata/releases/download/sing-box-ruleset/cnip.srs",
        "update_interval": "1d"
      }
    ],
    "final": "PROXY",
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "dns_direct"
    }
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "./zashboard",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "PROXY",
      "default_mode": "Rule"
    }
  }
}
